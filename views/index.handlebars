<div id="home">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        {{!-- <h1>{{msg}}</h1> --}}
        <h1>Have You Come To Challenge Me!? </h1>
        <form class="clearfix mb-4" action="POST">
          <div class="form-group">
            <label for="example-text">Competitor's Name</label>
            <input type="text" id="example-text" class="form-control" aria-describedby="example-text" placeholder="Enter User Name">
          </div>
          <div class="form-group">
            <label for="example-description"><h2>Choose Your Challenge</h2></label>
            
          </div>
          <button id="submit" class="btn btn-primary">Gaming</button>
          <button id="submit" class="btn btn-primary ">Excercise</button>
          <button id="submit" class="btn btn-primary ">TV Binge Watching</button>
          <button id="submit" class="btn btn-primary ">Misc Challenge</button>
        </form>
        
       <textarea class="form-control" id="example-description" aria-describedby="example-description" placeholder="Enter Your Challenge Data Here"></textarea>
       
        <br/>
         <h4> Competitor's Score Book: </h4>
        <ul id="example-list" class="list-group">
          {{#each examples}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="example/{{this.id}}">{{this.text}}</a>
            <button class="btn btn-danger float-right delete">ï½˜</button>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
